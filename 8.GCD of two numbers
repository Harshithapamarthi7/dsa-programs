#include <stdio.h>
int main() 
{
    int num1, num2, gcd;
    scanf("%d %d", &num1, &num2);
    while (num1 != num2) 
    {
        if (num1 > num2) 
        {
            num1 = num1 - num2;
        } 
        else 
        {
            num2 = num2 - num1;
        }
    }
    gcd = num1;
    printf("%d", gcd);
    return 0;
}
